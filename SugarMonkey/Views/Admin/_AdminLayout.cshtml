@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width"/>
    <title> : Admin Panel</title>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0">
    <link href="../../Artifacts/Old/CSS/admin_stylesheet.css" rel="stylesheet" type="text/css"/>
    <link href="../../Artifacts/Old/CSS/sweetalert.css" rel="stylesheet"/>
    @RenderSection("css", false)
</head>
<body>
<style>
    .LangDropDown { float: right; }

    .LangDropDown option a { color: green; }

    .displayLang { display: inline-block !important; }
</style>

<div class="main-cont">
    <!---->
    <div class="headertop">
        <div class="logo">
            <a href="/">Logo</a>
        </div>
        <div class="header-right">
            <span>
                Welcome @(Request.Cookies["MemberName"] != null ? Request.Cookies["MemberName"].Value : "") | <a href="/account/logout"> Log Out</a>
            </span>


        </div>
    </div>

    <div class="micCont">
        @Html.Partial("_AdminMenu")
        @RenderBody()
    </div>
</div>
<script src="../../Artifacts/Old/Scripts/jquery.min.js"></script>
<script src="../../Artifacts/Old/Scripts/sweetalert-dev.js"></script>
<script src="../../Artifacts/Old/Scripts/admin_function.js"></script>
<script src="../../Artifacts/Old/Scripts/CustomFunction.js"></script>
<script src="~/Scripts/jquery.validate.min.js"></script>
<script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
<script src="../../Artifacts/Old/Scripts/jquery.unobtrusive-ajax.js"></script>
<script src="../../Artifacts/Old/Scripts/jquery.dataTables.min.js"></script>
<link href="../../Artifacts/Old/CSS/jquery.dataTables.css" rel="stylesheet"/>
@RenderSection("js", false)
</body>
</html>